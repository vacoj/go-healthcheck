{
    "state": {
        "startup_state": false,
        "persist_state": true
    },
    "targets": [
        {
            "endpoint": "http://localhost:5704/fakehealth",
            "polling_interval": 5,
            "expected_status_code": 200
        },
        {
            "name": "FakeSmoke",
            "endpoint": "http://localhost:5704/fakesmoke",
            "polling_interval": 5,
            "expected_status_code": 200
        },
        {
            "name": "Expected Example",
            "endpoint": "http://localhost:5704/fakeexpected",
            "polling_interval": 5,
            "expected_status_code": 200,
